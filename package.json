{
  "name": "reso-certification",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "",
  "author": "BigBinary",
  "license": "MIT",
  "precommit": "lint-staged",
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "webpack-dev-server --inline --mode development --port 5678 --config webpack.dev.js --host 0.0.0.0",
    "dev:server": "nodemon --inspect=0.0.0.0:9229 --watch server server/bin/www",
    "qa": "run-s qa:client qa:server",
    "qa:client": "webpack --config webpack.qa.js",
    "qa:server": "node server/bin/www",
    "prod": "run-s prod:client prod:server",
    "prod:client": "webpack --config webpack.prod.js",
    "prod:server": "node server/bin/www",
    "migrate": "npx migrate",
    "precommit": "lint-staged",
    "test": "npx mocha --reporter spec"
  },
  "nodemonConfig": {
    "ignore": [
      "server/logs.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "{client,server,test}/**/*.{md,js,jsx,json}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
